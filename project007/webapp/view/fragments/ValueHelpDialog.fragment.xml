<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="valueHelpDialog"
		title="{i18n>SelectProduct}"
		items="{oData>/Categories}"
		search="handleValueHelpSearch"
		confirm="handleValueHelpClose"
		cancel="handleValueHelpClose"
		multiSelect="true">
		<StandardListItem
			iconDensityAware="false"
			iconInset="true"
			title="{oData>ID}" />
	</SelectDialog>
</core:FragmentDefinition>