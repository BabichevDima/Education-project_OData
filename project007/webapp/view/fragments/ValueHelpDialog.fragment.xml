<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="valueHelpDialog"
		title="{i18n>SelectProduct}"
		items="{/Categories}"
		search="openVHDialogSearch"
		confirm="openVHDialogClose"
		cancel="openVHDialogClose"
		multiSelect="true">
		<StandardListItem
			iconDensityAware="false"
			iconInset="true"
			title="{ID}" />
	</SelectDialog>
</core:FragmentDefinition>